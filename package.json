{
  "name": "payments.do",
  "version": "0.0.1",
  "description": "Accept payments today. Not next month. Stripe Connect as a managed service.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./sdk": {
      "types": "./dist/sdk.d.ts",
      "import": "./dist/sdk.js"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "build": "tsup",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "stripe": "^17.0.0",
    "rpc.do": "^0.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "tsup": "^8.5.1",
    "typescript": "^5.6.0",
    "vitest": "^3.2.4",
    "wrangler": "^4.0.0"
  },
  "author": { "name": "dot-do", "url": "https://payments.do" },
  "license": "MIT",
  "repository": { "type": "git", "url": "git+https://github.com/dot-do/payments.do.git" },
  "homepage": "https://payments.do"
}
